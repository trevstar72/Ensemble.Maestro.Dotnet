{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,1433;Database=MaestroDb;User Id=sa;Password=YourStrong!Passw0rd;TrustServerCertificate=True;",
    "Neo4j": "bolt://localhost:7687",
    "Elasticsearch": "http://localhost:9200",
    "Redis": "localhost:6379",
    "Qdrant": "http://localhost:6333"
  },
  "SemanticKernel": {
    "OpenAI": {
      "ApiKey": "your-openai-api-key-here",
      "ModelId": "gpt-4o"
    }
  },
  "Swarm": {
    "MaxConcurrentAgents": 10,
    "MaxAgentsPerProject": 50,
    "MaxMethodAgentsPerController": 8,
    "MaxControllers": 15,
    "ComplexityThresholdForMethodAgent": 4,
    "MaxCostPerProject": 100.00,
    "MaxAgentExecutionMinutes": 10,
    "MaxQueueDepth": 1000,
    "Throttling": {
      "MaxAgentsPerSecond": 2.0,
      "MaxAgentsPerMinute": 20,
      "MinSpawnIntervalMs": 500,
      "Enabled": true
    },
    "Retry": {
      "MaxRetryAttempts": 3,
      "InitialRetryDelaySeconds": 30,
      "RetryDelayMultiplier": 2.0,
      "MaxRetryDelaySeconds": 300,
      "RetryOnlyOnErrors": []
    },
    "ResourceLimits": {
      "Designer": {
        "MaxTokens": 4000,
        "MaxCostPerExecution": 5.00,
        "TimeoutMinutes": 5,
        "MaxMemoryMB": 512,
        "MaxCpuPercentage": 50
      },
      "UIDesigner": {
        "MaxTokens": 3000,
        "MaxCostPerExecution": 3.00,
        "TimeoutMinutes": 4,
        "MaxMemoryMB": 512,
        "MaxCpuPercentage": 50
      },
      "APIDesigner": {
        "MaxTokens": 3500,
        "MaxCostPerExecution": 4.00,
        "TimeoutMinutes": 5,
        "MaxMemoryMB": 512,
        "MaxCpuPercentage": 50
      },
      "CodeUnitController": {
        "MaxTokens": 2000,
        "MaxCostPerExecution": 2.00,
        "TimeoutMinutes": 8,
        "MaxMemoryMB": 512,
        "MaxCpuPercentage": 50
      },
      "MethodAgent": {
        "MaxTokens": 1500,
        "MaxCostPerExecution": 1.50,
        "TimeoutMinutes": 3,
        "MaxMemoryMB": 512,
        "MaxCpuPercentage": 50
      }
    },
    "Priority": {
      "HighPriorityAgentTypes": ["Designer", "APIDesigner"],
      "ComplexityPriorityBoost": 2,
      "UrgentPriorityBoost": 5,
      "MaxPriority": 10,
      "DefaultPriority": 5
    },
    "Health": {
      "HealthCheckIntervalSeconds": 30,
      "HeartbeatTimeoutSeconds": 120,
      "MinSuccessRatePercent": 80.0,
      "MaxAverageResponseTimeSeconds": 60,
      "MaxQueueBacklog": 500,
      "EnableAutoTermination": true
    },
    "AutoScaling": {
      "Enabled": true,
      "MinAgents": 1,
      "TargetQueueDepth": 10,
      "ScaleUpThreshold": 20,
      "ScaleDownThreshold": 5,
      "ScaleUpIncrement": 2,
      "ScaleDownIncrement": 1,
      "ScalingCooldownSeconds": 60,
      "CpuScaleUpThreshold": 70.0,
      "CpuScaleDownThreshold": 30.0
    }
  },
  "OutputPaths": {
    "RootDirectory": "D:\\maestro"
  },
  "AllowedHosts": "*"
}
